@model OrchardCore.DisplayManagement.Views.ShapeViewModel<OrchardCore.Contents.ViewModels.ContentOptionsViewModel>
@{
    var term = Model.Value.FilterResult.FirstOrDefault(x => x.TermName == "oib");
}

<h4 class="card-title">Oib</h4>

<pre class="mb-3">@(@term != null ? @term.ToString() : "oib:12345678901")</pre>
<p>@T["Filters on Person oib"]</p>
<div class="d-block text-right">
    <span>
        <i class="fas fa-sm fa-check text-primary" aria-hidden="true"></i>
    </span>
    <span >
        <i class="fas fa-sm fa-bars text-primary" aria-hidden="true"></i>
    </span>
</div>
