[
  {
    "copy": true,
    "inputs": [
      "Assets/dist/assets/*"
    ],
    "output": "wwwroot/assets/@"
  },
  {
    "copy": true,
    "inputs": [
      "Assets/dist/assets/img/*"
    ],
    "output": "wwwroot/assets/img/@"
  },
  {
    "inputs": [
      "Assets/dist/css/styles.css"
    ],
    "output": "wwwroot/css/styles.css"
  },
  {
    "generateSourceMaps": false,
    "inputs": [ "Assets/src/scss/bootstrap-oc.scss" ],
    "output": "wwwroot/css/bootstrap-oc.css"
  },
  {
    "inputs": [
      "Assets/dist/js/scripts.js"
    ],
    "output": "wwwroot/js/scripts.js"
  }
]
